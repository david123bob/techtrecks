{
  "manifest_version":3,
  "name":"Food analyzer",
  "version":"1.0.0",
  "description": "this is a extension to get data on the food/items that you buy",
  "author":"me",
  "content_scripts":[
    {
      "matches":["https://www.amazon.com/*"],
      "js":["script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action":{
    "default_popup":"index.html",
    "default.title":"Food analyzer"
  }
}